<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MultiStateView">
        <attr name="msv_loadingView" format="reference" />
        <attr name="msv_noNetView" format="reference" />
        <attr name="msv_emptyView" format="reference" />
        <attr name="msv_errorView" format="reference" />
        <attr name="msv_customView" format="reference" />
        <attr name="msv_viewState" format="enum">
            <enum name="content" value="0" />
            <enum name="noNet" value="1" />
            <enum name="error" value="2" />
            <enum name="empty" value="3" />
            <enum name="loading" value="4" />
            <enum name="custom" value="5" />
        </attr>
        <attr name="msv_animateViewChanges" format="boolean" />
    </declare-styleable>

</resources>